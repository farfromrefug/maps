    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^4.0.0",
    "globals": "^15.14.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "redux-devtools-extension": "^2.13.8",
    "typescript-eslint": "^7.18.0"
  },
  "packageManager": "yarn@4.3.1"
